<h2>project_edit.erb</h2><br><br>


<h1>Update the <%=@Projects.name_project() %> list </h1>

<form action="/projects/<%= @projects.id() %>" method="post">
  <input name="_method" type="hidden" value="patch">

  <label for="name_project">Rename your project:</label>
  <input id="name_project" name="name_project" type="text">

  <button type="submit">Update</button>
</form>
